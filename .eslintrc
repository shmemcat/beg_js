{
   "extends": [
      "wesbos"
   ],
   "rules": {
      "no-unused-vars": "off",
      "quotes": [
         2,
         "double"
      ],
      "prettier/prettier": [
         "error",
         {
            "semi": true,
            "tabWidth": 3,
            "singleQuote": false,
            "quoteProps": "preserve"
         }
      ]
   }
}